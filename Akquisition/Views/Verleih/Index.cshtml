@model IEnumerable<efAkquisition.tbl_Verleih>

@{
    ViewBag.Title = "Verleiher";
}

<h2>Verleiher</h2>

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
        @Html.ActionLink("Neuen Verleiher erstellen", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Verleih)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Verleih)
            </td>
            <td>
                @Html.ActionLink("Bearbeiten", "Edit", new { id=item.VerleihNr }) |
                @Html.ActionLink("Löschen", "Delete", new { id=item.VerleihNr })
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
